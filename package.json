{
    "name": "trail-sc-poc",
    "version": "0.0.1",
    "description": "Trails Proof of Concept",
    "main": "index.js",
    "type": "module",
    "scripts": {
      "test": "jest",
      "build": "run-s clean prettier lint compile copy",
      "lint": "tslint -c tslint.json 'src/**/*.ts'",
      "lint-fix": "tslint -c tslint.json --fix 'src/**/*.ts'",
      "compile": "tsc",
      "clean": "rimraf './es/*' './typings/*'",
      "copy": "copyfiles -u 1 src/**/*.bytecode es/",
      "prettier": "prettier --config .prettierrc 'src/**/*' --write",
      "start": "node --es-module-specifier-resolution=node ./es/index.js"
    },
    "author": "IOTA Team",
    "license": "ISC",
    "devDependencies": {
      "ts-node": "^10.9.1",
      "typescript": "^5.2.2",
      "rimraf": "^5.0.5",
      "copyfiles": "^2.4.1",
      "npm-run-all": "4.1.5",
      "@types/jest": "^29.5.5",
      "@types/node": "^20.7.1",
      "jest": "^29.7.0",
      "ts-jest": "^29.1.1",
      "tslint": "^6.1.3",
      "prettier": "^3.0.3"
    },
    "dependencies": {
      "ethers": "^6.7.1",
      "dotenv": "^16.0.3",
      "dotenv-expand": "^10.0.0",
      "tslog": "^4.9.2"
    }
  }