{
    "compilerOptions": {
      "target": "esnext",
      "module": "es2020",
      "moduleResolution": "node",
      "lib": [
        "dom",
        "es2015",
        "es2015.iterable"
      ],
      "esModuleInterop": true,
      "noUnusedLocals": true,
      "outDir": "./es",
      "resolveJsonModule": true,
      "inlineSourceMap": true,
      "declaration": true,
      "declarationDir": "typings",
      "stripInternal": true,
      "importHelpers": true,
      "forceConsistentCasingInFileNames": true,
      "isolatedModules": true,
      "allowJs": true
    },
    "include": [
      "src"
    ]
  }
  